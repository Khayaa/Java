<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="595.0" prefWidth="911.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="letsplay.DashboardController">
   <children>
      <TableView fx:id="usertable" layoutX="343.0" layoutY="61.0" prefHeight="410.0" prefWidth="542.0">
        <columns>
          <TableColumn fx:id="idcol" prefWidth="64.0" text="ID" />
            <TableColumn fx:id="namecol" prefWidth="145.5999755859375" text="NAME" />
            <TableColumn fx:id="surnamecol" prefWidth="150.39996337890625" text="LASTNAME" />
            <TableColumn fx:id="emailcol" prefWidth="182.39996337890625" text="EMAIL" />
        </columns>
      </TableView>
      <VBox layoutX="35.0" layoutY="70.0" prefHeight="410.0" prefWidth="290.0">
         <children>
            <TextField fx:id="id_" prefHeight="46.0" prefWidth="270.0" promptText="ID" />
            <TextField fx:id="name_" prefHeight="45.0" prefWidth="290.0" promptText="Name" />
            <TextField fx:id="lastname_" prefHeight="45.0" prefWidth="290.0" promptText="Lastname" />
            <TextField fx:id="email_" prefHeight="45.0" prefWidth="290.0" promptText="Email" />
            <Button fx:id="savebtn" mnemonicParsing="false" onAction="#Save" prefHeight="66.0" prefWidth="288.0" text="SAVE">
               <font>
                  <Font name="Copperplate Gothic Light" size="21.0" />
               </font></Button>
            <Button fx:id="deletebtn" mnemonicParsing="false" onAction="#Delete" prefHeight="66.0" prefWidth="288.0" text="DELETE">
               <font>
                  <Font name="Copperplate Gothic Light" size="21.0" />
               </font></Button>
            <Button fx:id="clearbtn" mnemonicParsing="false" onAction="#Clear" prefHeight="66.0" prefWidth="288.0" text="CLEAR">
               <font>
                  <Font name="Copperplate Gothic Light" size="21.0" />
               </font></Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
